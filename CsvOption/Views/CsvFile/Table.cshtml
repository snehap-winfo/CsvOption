@model IEnumerable<CsvOption.Models.CsvRow>
@using System.Linq
@{
    ViewData["Title"] = "Table";
}

<h1 align="center">Table</h1>


@if ((Model?.Count() ?? 0) > 0)
{
    <table align="center" border="1">

        <thead>
           @foreach(var header in Model.First().Fields.Keys)

            {
                <th>@header</th>
            }

        </thead>

    <tbody>
        @foreach(var row in Model)
        {
            <tr>
                @foreach(var value in row.Fields.Values)
                {
                        <td>@value</td>
                }
            </tr>
        }


    </tbody>

</table>
}


